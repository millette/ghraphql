{
  "name": "ghraphql",
  "description": "Search GitHub for users by location.",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "cli.js",
  "license": "AGPL-3.0",
  "repository": "millette/ghraphql",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "keywords": [
    "github",
    "graphql",
    "users",
    "location",
    "cli"
  ],
  "lint-staged": {
    "*.{js,json,md,graphql}": [
      "prettier-standard",
      "git add"
    ]
  },
  "scripts": {
    "deps": "dependency-check . && dependency-check . --unused --no-dev",
    "lint": "standard",
    "format": "prettier-standard '**/*.+(js|json|md|graphql)'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "debug": "^4.1.1",
    "delay": "^4.3.0",
    "franc-min": "^4.1.0",
    "get-best-contrast-color": "^0.3.0",
    "got": "^9.6.0",
    "lodash.countby": "^4.6.0",
    "lodash.deburr": "^4.1.0",
    "lodash.difference": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.intersectionwith": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.words": "^4.2.0",
    "marked": "^0.7.0",
    "marked-terminal": "^3.2.0",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "normalize-email-or-url": "^1.0.0",
    "p-map": "^2.1.0",
    "progress": "^2.0.3",
    "rollodeqc-gh-user-streak": "^4.0.0",
    "stopwords": "^0.0.9",
    "weeknumber": "^1.1.1"
  },
  "devDependencies": {
    "dependency-check": "^3.4.1",
    "eslint": "^6.0.1",
    "husky": "^2.7.0",
    "lint-staged": "^9.2.0",
    "prettier-standard": "^9.1.1",
    "standard": "^13.0.2"
  }
}
